<?xml version="1.0" encoding="UTF-8"?>
<config>
    <global>
        <blocks>
            <catalog>
                <rewrite>
                    <product_view>Day_Two_Block_Catalog_Product_View</product_view>
                </rewrite>
            </catalog>
            <page>
                <rewrite>
                    <html_breadcrumbs>Day_Two_Block_Htmlbreadcrumbs</html_breadcrumbs>
                </rewrite>
            </page>
            <day_two>
                <class>Day_Two_Block</class>
            </day_two>
        </blocks>
        <helpers>
            <day_two>
                <class>Day_Two_Helper</class>
            </day_two>
        </helpers>
        <models>
            <day_two>
                <class>Day_Two_Model</class>
            </day_two>
        </models>
    </global>
    
    <frontend>
        <routers>
            <day_two>
                <use>standard</use>
                <args>
                    <module>Day_Two</module>
                    <frontName>heyo</frontName>
                </args>
            </day_two>
        </routers>
    </frontend>
    
</config>
